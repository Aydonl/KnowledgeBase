alpine3.4系统编译安装tengin

 echo -e 'http://mirrors.aliyun.com/alpine/v3.4/main\nhttp://mirrors.aliyun.com/alpine/v3.4/community' /etc/apk/repositories

apk add pcre pcre-dev openssl-dev gcc make libc-dev zlib-dev linux-headers jemalloc-dev --no-cache


wget http://192.168.66.178:8080/nginx/tengine-2.1.2.tar.gz
wget http://192.168.66.178:8080/nginx/ngx_cache_purge-2.3.tar.gz
wget https://github.com/ChasonTang/docker-library/blob/master/tengine/ngx_user.patch

tar xf tengine-2.1.2.tar.gz
tar xf ngx_cache_purge-2.3.tar.gz
cd tengine-2.1.2
./configure \
--prefix=/usr/local/nginx \
--with-http_stub_status_module \
--with-pcre \
--sbin-path=/usr/sbin/nginx \
--conf-path=/etc/nginx/nginx.conf \
--error-log-path=/var/log/nginx/error.log \
--http-log-path=/var/log/nginx/access.log \
--pid-path=/var/run/nginx/nginx.pid \
--lock-path=/var/lock/nginx.lock \
--user=nginx \
--group=nginx \
--with-http_ssl_module \
--with-http_flv_module \
--with-http_gzip_static_module \
--with-file-aio \
--with-http_v2_module \
--with-http_realip_module \
--with-http_addition_module \
--with-http_sub_module \
--with-http_dav_module \
--with-http_flv_module \
--with-http_slice_module \
--with-http_mp4_module \
--with-http_gunzip_module \
--with-http_auth_request_module \
--with-http_concat_module \
--with-http_random_index_module \
--with-http_secure_link_module \
--with-http_degradation_module \
--with-http_sysguard_module \
--with-http_dyups_module \
--http-proxy-temp-path=/kuaiyun/proxy/ \
--with-ipv6 \
--add-module=../ngx_cache_purge-2.3 \
--with-jemalloc \